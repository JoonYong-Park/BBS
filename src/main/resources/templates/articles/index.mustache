{{>layouts/header}}

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Title</th>
            <th scope="col">Content</th>
        </tr>
    </thead>
    <tbody>
        {{#articleList}} <!-- 여러 데이터의 묶음이면 반복실행됨 -->
            <tr>
                <th>{{id}}</th>
                <td>{{title}}</td>
                <td>{{content}}</td>
            </tr>
        {{/articleList}}
    </tbody>
</table>

{{>layouts/footer}}